#include <stdio.h>

int main() {
    int num, i = 1, product;

    printf("Enter a number: ");
    scanf("%d", &num);

    printf(" %d ", num);

    while (1) {
        product = num * i;
        if (product >= 100) {
            printf("%d * %d = %d\n", num, i, product);
            break;
        }
        printf("%d * %d = %d\n", num, i, product);
        i++;
    }

    return 0;
}
